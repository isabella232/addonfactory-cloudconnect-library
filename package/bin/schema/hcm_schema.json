            {
              "type": "string"
            }
          ]
        },
        "host": {
          "type": "string"
        },
        "port": {
          "oneOf": [
            {
              "type": "integer"
            },
            {
              "type": "string"
            }
          ]
        },
        "username": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
