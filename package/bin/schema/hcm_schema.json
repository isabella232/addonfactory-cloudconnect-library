            "GET",
            "POST",
            "PUT",
            "DELETE",
            "PATCH"
          ]
        },
        "stop_condition": {
          "type": "string",
          "enum": [
            "empty",
            "once"
          ],
          "default": "empty"
        },
        "output": {
          "type": "array",
          "items": {
            "oneOf": [
              {
                "$ref": "#/definitions/hcm_json_schema_builder._StdoutOutput"
