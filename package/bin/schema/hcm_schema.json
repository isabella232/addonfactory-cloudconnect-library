        "data": {
          "type": "object",
          "properties": {
            "source_key": {
              "type": "string"
            },
            "format": {
              "type": "string",
              "enum": [
                "splunk_xml",
                "raw"
              ]
            }
          },
          "required": [
            "source_key",
            "format"
          ],
          "additionalProperties": true
        },
        "type": {
          "type": "string",
          "enum": [
            "file"
          ]
        },
        "options": {
          "type": "object"
        }
      },
      "required": [
        "data",
        "options"
      ],
      "additionalProperties": true
    },
    "hcm_json_schema_builder._Proxy": {
      "type": "object",
      "properties": {
        "enabled": {
          "oneOf": [
            {
              "type": "boolean"
